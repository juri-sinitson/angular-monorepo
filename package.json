{
  "name": "@angular-monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start-frontend": "nx serve examples-frontend",
    "start-combined": "start-server-and-test ci:helper:start-backend http://localhost:3000 start-frontend",
    "start-backend": "pnpm ci:helper:start-backend",
    "storybook": "NODE_ENV=development pnpm nx run non-prod-storybook:storybook",
    "storybook-alt": "pnpm nx run non-prod-storybook:storybook",
    "ci:lint-test-build": "NX_BRANCH=$(git branch --show-current) pnpm nx affected -t lint test build --parallel",
    "ci:helper:start-backend": "nx run central-backend:build && node dist/apps/central-backend/main",
    "ci:helper:e2e": "NX_BRANCH=$(git branch --show-current) pnpm nx affected -t e2e --parallel",
    "ci:e2e": "pnpm start-server-and-test ci:helper:start-backend http://localhost:3000 ci:helper:e2e",
    "ci:helper:storybook": "pnpm storybook -- --ci",
    "ci:helper:test-storybook": "pnpm exec jest --clearCache -u && NX_BRANCH=$(git branch --show-current) pnpm nx affected -t test-storybook --parallel --url http://localhost:4400",
    "ci:test-storybook": "pnpm start-server-and-test ci:helper:storybook http://localhost:4400 ci:helper:test-storybook",
    "spot-check": "bash ./spot-check.sh",
    "container:stop": "docker-compose rm -fsv container",
    "container:prune": "docker system prune",
    "container:rebuild": "pnpm container:stop && docker build -t frontend-juri-sinitson/image:dev --no-cache . && docker image prune -f",
    "container:start": "docker-compose up -d container",
    "container:enter": "pnpm container:start && docker-compose exec container bash",
    "container:exec": "pnpm container:start && docker-compose exec container bash -c",
    "container:lint-test-build": "pnpm container:exec -- \"NX_CLOUD_ACCESS_TOKEN=$(cat nx-cloud-access-token) pnpm ci:lint-test-build\"",
    "container:e2e": "pnpm container:exec -- \"NX_CLOUD_ACCESS_TOKEN=$(cat nx-cloud-access-token) pnpm ci:e2e\"",
    "container:test-storybook": "pnpm container:exec -- \"NX_CLOUD_ACCESS_TOKEN=$(cat nx-cloud-access-token) pnpm ci:test-storybook\""
  },
  "private": true,
