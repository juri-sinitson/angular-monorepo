  "scripts": {
    "start-backend": "nx run examples-backend:build && node dist/apps/examples-backend/main",
    "start-frontend": "nx serve examples-frontend",
    "start-combined": "start-server-and-test start-backend http://localhost:3000 start-frontend",
    "storybook": "pnpm nx run non-prod-storybook:storybook",
    "storybook-alt": "NODE_ENV=development pnpm nx run non-prod-storybook:storybook",
    "test-storybook": "pnpm nx affected --verbose -t test-storybook --parallel --url http://localhost:4400",
    "cy:run": "pnpm nx affected --verbose -t e2e --parallel",
    "cy:run-headed": "pnpm nx run --verbose examples-frontend-e2e:e2e --headed --watch --browser chrome",
    "e2e-headed": "pnpm start-server-and-test start-backend http://localhost:3000 cy:run-headed",
    "ci:storybook": "pnpm storybook -- --ci",
    "ci:e2e": "pnpm start-server-and-test start-backend http://localhost:3000 cy:run",
    "ci:test": "pnpm nx affected --verbose -t test --parallel",
    "ci:lint": "pnpm nx affected --verbose -t lint --parallel",
    "ci:lint-test-build": "pnpm nx affected --verbose -t lint test build --parallel",
    "ci:test-storybook": "pnpm start-server-and-test ci:storybook http://localhost:4400 test-storybook"
  },
